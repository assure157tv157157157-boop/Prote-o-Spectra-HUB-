-- Executa os dois scripts do Spectra HUB ao mesmo tempo

local urls = {
    "https://raw.githubusercontent.com/assure157tv157157157-boop/Spectra-HUB-/refs/heads/main/Painel%20ADM%20do%20spectra%20hub",
    "https://raw.githubusercontent.com/assure157tv157157157-boop/Spectra-HUB-/refs/heads/main/Come%C3%A7o%20e%20URL%20do%20Spectra"
}

for _, url in ipairs(urls) do
    task.spawn(function()
        local success, err = pcall(function()
            loadstring(game:HttpGet(url))()
        end)
        if not success then
            warn("Erro ao executar script:", err)
        end
    end)
end
