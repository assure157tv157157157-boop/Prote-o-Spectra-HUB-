-- Proteção para executar apenas uma vez
if _G.SpectraHubLoaded then
    warn("Spectra Hub já foi executado.")
    return
end
_G.SpectraHubLoaded = true

-- URLs dos scripts
local scripts = {
    "https://raw.githubusercontent.com/assure157tv157157157-boop/Spectra-HUB-/refs/heads/main/Painel%20ADM%20do%20spectra%20hub",
    "https://raw.githubusercontent.com/assure157tv157157157-boop/Spectra-HUB-/refs/heads/main/Come%C3%A7o%20e%20URL%20do%20Spectra"
}

-- Executa ambos apenas uma vez
for _, url in ipairs(scripts) do
    task.spawn(function()
        local ok, err = pcall(function()
            loadstring(game:HttpGet(url))()
        end)
        if not ok then
            warn("Erro ao carregar:", url, err)
        end
    end)
end
